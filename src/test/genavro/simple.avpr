protocol Simple {
  enum Kind {
    FOO,
    BAR,
    BAZ
  }

  fixed MD5(16);

  record TestRecord {
    @order("ignore")
    string name;

    @order("descending")
    Kind kind;

    MD5 hash;
  }

  error TestError {
    string message;
  }

  string hello(string greeting);
  TestRecord echo(TestRecord zrecord); // TODO: record is a reserved word
  int add(int arg1, int arg2);
  bytes echoBytes(bytes data);
  void doerror() throws TestError; // TODO error reserved word
}
